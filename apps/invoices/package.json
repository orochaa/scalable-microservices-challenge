{
  "name": "invoices",
  "description": "Nest TypeScript starter repository",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "src/main/server.ts",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "lint": "run-s lint:tsc lint:prettier lint:eslint",
    "lint:eslint": "eslint --fix \"src/**/*.ts\"",
    "lint:prettier": "prettier --write .",
    "lint:tsc": "tsc --noEmit",
    "prisma:dev": "prisma generate && prisma migrate dev",
    "prisma:seed": "tsx playground/prisma-seed.ts",
    "prisma:prod": "prisma generate && prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "start": "node dist/main/server.js",
    "ci": "run-s prisma:dev lint test"
  },
  "dependencies": {
    "@workspace/contracts": "./contracts",
    "@nestjs/common": "^11.0.6",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.6",
    "@nestjs/microservices": "^11.1.2",
    "@nestjs/platform-express": "^11.0.6",
    "@nestjs/swagger": "^11.0.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.0",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@prisma/client": "^6.2.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@swc/core": "^1.10.11",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "eslint": "^9.19.0",
    "eslint-plugin-mist3rbru": "^0.6.4",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "source-map-support": "^0.5.21",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@prisma/client",
      "@prisma/engines",
      "@swc/core",
      "esbuild",
      "prisma"
    ],
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@^1",
      "array.prototype.findlast": "npm:@nolyfill/array.prototype.findlast@^1",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@^1",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@^1",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@^1",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@^1",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@^1",
      "hasown": "npm:@nolyfill/hasown@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "object.assign": "npm:@nolyfill/object.assign@^1",
      "object.entries": "npm:@nolyfill/object.entries@^1",
      "object.fromentries": "npm:@nolyfill/object.fromentries@^1",
      "object.values": "npm:@nolyfill/object.values@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1",
      "side-channel": "npm:@nolyfill/side-channel@^1",
      "string.prototype.includes": "npm:@nolyfill/string.prototype.includes@^1",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
      "typedarray": "npm:@nolyfill/typedarray@^1"
    }
  }
}
